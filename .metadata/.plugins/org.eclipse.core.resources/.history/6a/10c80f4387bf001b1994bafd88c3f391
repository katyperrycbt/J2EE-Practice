<?xml encoding="UTF-8"?>

<!ELEMENT book (bookdetails)>
<!ATTLIST book
  xmlns CDATA #FIXED ''>

<!ELEMENT bookdetails (isbn,title,author,publisher,publicationdate,
                       price)>
<!ATTLIST bookdetails
  xmlns CDATA #FIXED ''>

<!ELEMENT isbn (#PCDATA)>
<!ATTLIST isbn
  xmlns CDATA #FIXED ''>

<!ELEMENT title (#PCDATA)>
<!ATTLIST title
  xmlns CDATA #FIXED ''>

<!ELEMENT author (#PCDATA)>
<!ATTLIST author
  xmlns CDATA #FIXED ''>

<!ELEMENT publisher (#PCDATA)>
<!ATTLIST publisher
  xmlns CDATA #FIXED ''>

<!ELEMENT publicationdate (#PCDATA)>
<!ATTLIST publicationdate
  xmlns CDATA #FIXED ''>

<!ELEMENT price (#PCDATA)>
<!ATTLIST price
  xmlns CDATA #FIXED ''>


